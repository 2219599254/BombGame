/* pages/distri_page/distri_page.wxss */
@import "/common/common.wxss";
.container{
    position: relative;
    height: 100%;
}
.my_container{
    width: 100%;
    height: 100%;
}
.input_box .title{
    padding: 20rpx 24rpx;
    font-size: 28rpx;
    color: #666;
}
.input_box .inputs{
    background-color: #FFF;
    padding-left: 24rpx;
}
.input_box .name{
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 2rpx solid #e7e7e7;
}
.input_box .name span{
    font-size: 32rpx;
    padding-left: 10rpx;
}
.input_box .phone{
    height: 100rpx;
    line-height: 100rpx;
}
.input_box label{
    float: left;
    width: 150rpx;
    color: #333;
    font-size: 32rpx;
}
.input_box input{
  font-size: 32rpx;
  color: #333; 
  padding-left:10rpx;
}

.btn{
  width: 680rpx;
  /* background: #ff3939; */
  color: #fff;
  height: 100rpx;
  padding-top: 20rpx;
  font-size: 28rpx;
  text-align: center;
  margin: 0 auto;
  margin-top: 68rpx;
  margin-bottom: 64rpx;
}
.btn.addclass{
    width: 558rpx;
    height: 88rpx;
    line-height: 88rpx;
    padding-top: 0;
}
.notice{
    padding-left: 68rpx;
}
.notice.addclass{
    background-color: #FFF;
    padding: 28rpx 0 28rpx 68rpx;
}
.notice ._title{
    font-size: 36rpx;
    color: #333;
    position: relative;
}
._title image{
    width:32rpx;
    height:26rpx;
    position: absolute;
    top: 8rpx;
    left: -44rpx;
}
.notice .title{
    font-size: 28rpx;
    color: #333;
    margin-bottom: 10rpx;
    margin-top: 24rpx;
}
.notice .span{
    font-size: 24rpx;
    color: #999;
    box-sizing: border-box;
    width: 100%;
    padding-right: 80rpx;
}
.notice .span.span2{
    padding-bottom: 32rpx;
    border-bottom: 2rpx solid #e7e7e7
}
.notice .span.span3{
    padding-top: 16rpx;
}
.bottom_notice{
    height: 100rpx;
    width: 100%;
    text-align: center;
    font-size: 24rpx;
    position: absolute;
    bottom: 0;
    color: #ccc;
}
.my_position_box{
    position: relative;
    height: 100%;
}
.my_position{
    position: absolute;
    top: 50%;
    left: 50%;
    overflow: hidden;
    -webkit-transition-property: -webkit-transform,opacity;
    transition-property: transform,opacity;
    transform: translate(-50%, -50%);
}

.applied{
    position: relative;
}
.get_help{
    text-align: center;
    color: #ff3939;
    font-size: 24rpx;

    position: absolute;
    bottom: -74rpx;
    right: 60rpx;
    padding-top: 24rpx;
}
/*入驻后默认显示页  */
.submit_default{
    margin-top: 90rpx;
    padding: 24rpx;
}
.tishi_box{
    height: 400rpx;
    text-align: center;
    padding-top: 280rpx;
}
.tishi_box .text{
    color: #999;
    font-size: 28rpx;
    margin-top: 14rpx;
}
.my_notice{
    color: #0b68f4;
    font-size: 28rpx;
    padding: 20rpx 0;
}





