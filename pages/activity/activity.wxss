/* pages/activity/activity.wxss */
page{
  width: 100%;
}
.container{
  width: 100%;
  height: 100%;
  position: relative;
}
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
.success_notice_box{
  width: 590rpx;
  height: 369rpx;
  background-size:100%;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 1000;
  overflow: hidden;
  -webkit-transition-property: -webkit-transform,opacity;
  transition-property: transform,opacity;
  transform: translate(-50%, -50%);
}
.success_notice_box .find_people_help{
  margin-top: 48rpx;
  position: relative;
  text-align: center;
  color: #FFF;
  height: 100%;
}
.success_notice_box image{
  position: absolute;
  top: 14rpx;
  right: 40rpx;
}
.btn_take{
    position: absolute;
    bottom: 94rpx;
    right: 170rpx;
    border-radius: 8rpx;
    width: 216rpx;
    height: 64rpx;
    line-height: 68rpx;
    font-size: 24rpx;
    text-align: center;
    background-color: #ffea00;
    color: #FB1B1B;
}

/*1111  */
.my_container{
  background-color: #F7F7F7;
  width: 100%;
}
.banner{
  font-size: 0;
}
.banner image{width: 100%;height: 350rpx;}

.title {
  height: 60rpx;
  line-height: 60rpx;
  display: inline-flex;
  margin-top: 20rpx;
  width: 750rpx;
}

.title .a1 {
  width: 5rpx;
  height: 32rpx;
  background-color: #f00;
  margin: 12rpx 18rpx 0 24rpx;
}

.title .a2 {
  width: 480rpx;
  font-size: 28rpx;
}

.title .a3 {
  color: #666;
  margin-right: 16rpx;
  font-size: 28rpx;
}

.title .a4 image {
  width: 12rpx;
  height: 19rpx;
}

.newss{padding: 0 24rpx;box-sizing: border-box;}

.news {
  margin: 20rpx 0 20rpx 0;
  width: 702rpx;
  height: 146rpx;
  display: flex;
  background-color: #fff;
  border-radius: 10rpx;
}

.news .left {
  width: 205rpx;
}

.news .right {
  width: 480rpx;
}
.news .b_title:after{
  content:"";
  height:0;
  line-height:0;
  display:block;
  visibility:hidden;
  clear:both;
}
.news .b_title {
  padding-top: 20rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.b_title_name{
  width: 60%;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  display: inline-block;
}

.news .b_line {
  display: inline-flex;
  justify-content: space-between;
  color: #999;
  height: 36rpx;
}

.news .b_line .yd {
  width: 360rpx;
  font-size: 24rpx;
  color: #666;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  display: inline-block;
}

.news .c_line .yd {
  color: #ff0000;
  font-size: 32rpx;
  width: 334rpx;
  margin-top:-10rpx;
}

.news .b_line .rq {
  width: 156rpx;

}

.news .c_line .rq{
  width:148rpx;
  height: 44rpx;
  background-color: #ff0000;
  color: #fff;
  text-align: center;
  border-radius: 20rpx;
  margin-top:-15rpx;
  font-size: 24rpx;
  line-height: 44rpx;
}
.news .c_line .rq.zanting,.news .c_line .rq.zanting{
  background-color: #eee;
}

.news image {
  width: 162rpx;
  height: 118rpx;
  margin: 14rpx 28rpx 0 14rpx;
  border-radius: 12rpx;
}
.notice_box{
  padding: 110rpx 24rpx 40rpx;
}
.notice{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  border:2px solid #ff0000;
  border-radius:12rpx; 
  background-color: #fff;
  text-align: center;
}

.notice .aa{
  margin: 48rpx 40rpx;
  text-align: left;
}

.notice .aa text{
  color: #666;
  font-size: 24rpx;
}

.notice .ab{
  width: 450rpx;
  height: 74rpx;
  margin-top:-30rpx;
}