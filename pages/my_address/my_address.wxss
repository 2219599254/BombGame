/* pages/my_address/my_address.wxss */
.container{
  height: 100%;
}
.my_container{
    width: 100%;
    margin-top: 2rpx;
}
.inp:after,.inp_1:after{
    clear:both;
    content:'.';
    display:block;
    width: 0;
    height: 0;
    visibility:hidden;
}
.inp{
  padding-left: 24rpx;
  background: #fff;
}
.inp_1{
  border-bottom:1px solid rgba(153,153,153,0.1);
  background: #fff;
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.inp_1 label{
    float: left;
    color: #333;
    font-size: 28rpx;
}
.inp_1 input{
  float: right;
  width: 540rpx;
  height: 100rpx;
  text-align: right;
  font-size: 28rpx;
  color: #333;
  padding-right: 24rpx;
}
.btn{
  /* background: #ff3939; */
  color: #fff;
  margin: 126rpx 10rpx 26rpx 10rpx;
  height: 100rpx;
  padding-top: 20rpx;
  font-size: 28rpx;
  text-align: center;
}
button::after {border:0;}

.mapAddress,.brand{
    position: absolute;
    right: 24rpx;
    display: inline-block;
    width:450rpx;
    text-align: right;
    overflow:hidden;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    -webkit-text-overflow:ellipsis;
    -moz-text-overflow:ellipsis;
    white-space:nowrap;
    color: #333;
    font-size: 28rpx;
}