.search_box {
  width: 702rpx;
  height: 68rpx;
  line-height: 68rpx;
  background-color: #fff;
  border-radius: 10rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  overflow: hidden;
  -webkit-transition-property: -webkit-transform, opacity;
  transition-property: transform, opacity;
  transform: translate(-50%, -50%);
}

.search_box image {
  position: relative;
  top: 8rpx;
  margin-right: 12rpx;
}

