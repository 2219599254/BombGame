/* pages/my_coupon/my_coupon.wxss */
.container{
    background-color:#f9f9f9;
    height: 100%;
}
.my_container{
    width: 100%;
    position: relative;
    padding-bottom: 130rpx;
}
.aaaaa:after,.information:after,.title2:after{
    clear:both;
    content:'.';
    display:block;
    width: 0;
    height: 0;
    visibility:hidden;
}
.code_list{
    margin: 24rpx 24rpx 0;
    height: 230rpx;
    background-color: #FFF;
    box-shadow:0rpx 2rpx 2rpx #eee;
    background-size:100%;
}
.code_list2{
    margin: 36rpx 24rpx 0;
    height: 230rpx;
    background-color: #FFF;
    background-size:100%;
}
.view_top{
    height: 172rpx;
    color: #FFF;
    position: relative;
}
.aaaaa{
    padding: 40rpx 0 0 24rpx;
    font-size: 0;
}
.view_top .aaaaa .money{
    width: 196rpx;
    height: 102rpx;
    line-height: 102rpx;
    float: left;
}
.view_top .name{
    font-size: 24rpx;
    margin-bottom: 10rpx;
}
.view_top .coupon{
    font-size: 32rpx;
}
/*领没领，用没用  */
.overdue{
    position: absolute;
    top: 0;
    right: 44rpx;
}
.overdue image{
    width: 167rpx;
    height: 152rpx;
}
/*立即使用  */
.btn_take{
    position: absolute;
    top: 64rpx;
    right: 24rpx;
    border: 2rpx solid #FFF;
    border-radius: 8rpx;
    width: 146rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 24rpx;
    text-align: center;
}
.btn_take .box{
    display: inline-block;
    /* padding: 6rpx 18rpx; */
}

.cont{
    display: inline-block;
    position: absolute;
    top:68rpx;
}
.view_bottom{
    height: 58rpx;
    color: #9c9c9c;
    font-size: 24rpx;
    padding: 0 20rpx;
}
.float_left{
    float: left;
    height: 58rpx;
    line-height: 58rpx;
}
.float_right{
    float:right;
    height: 58rpx;
    line-height: 58rpx;
}

.default_text{
    text-align: center;
    margin-top: 410rpx;
    font-size: 28rpx;
    color: #666;
}

.receive_more_coupon{
    position: fixed;
    bottom: 0;
    left: 0;

    width: 100%;
    height: 98rpx;
    line-height: 98rpx;
    text-align: center;
    /* background-color: #ff3939; */
    color: #FFF;
    font-size: 28rpx;
}